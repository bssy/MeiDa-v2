<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="com.baidu.mobads.AdView">
        <attr name="adSize" format="integer" />
        <attr name="adId" format="string" />
    </declare-styleable>
    
    <declare-styleable name="CircleImageView">
        <attr name="border_width_ci" format="dimension" />
        <attr name="border_color_ci" format="color" />
    </declare-styleable>

    <declare-styleable name="apptitlebar">
        <attr name="leftIcon" format="reference" />
        <attr name="rightIcon" format="reference" />
        
        <attr name="leftStringColor" format="color"/>
        <attr name="rightStringColor" format="color"/>
        
        <attr name="leftString" format="string"/>
        <attr name="rightString" format="string"/>
        
        <attr name="titlebarString" format="string"></attr>
        <attr name="titlebarStringColor" format="color"></attr>
    </declare-styleable>
    
</resources>